Node(Head) {
    map<string, int> db = {};

    func Write(key, value) -> none {
        db[key] = value;
        return true;
    }

    func Read(key) -> int {
        return db[key];
    }
}

ClientInterface {
    func write(dest, key, value) -> none {
        rpc_call(dest, Write(key, value));
    }

    func read(dest, key) -> int {
        return rpc_call(dest, Read(key));
    }
}